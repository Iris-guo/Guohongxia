<script>
        var chart = echarts.init(document.getElementById('main'));
        var wordtext1="<?php echo $wordtext;?>"
        var wordtext=wordtext1.split(',');
        console.log(wordtext);
        var wordweight1="<?php echo $wordweight;?>"
        var wordweight=wordweight1.split(',');

        var keywords1="<?php echo $keywords;?>"
        var keywords=keywords1.split(',');
        console.log(keywords);
        var data=[];
        for(var key in wordtext){
          data.push({
            name:wordtext[key],
            value:wordweight[key],
          })
        }
        var maskImage=new Image();
        // maskImage.src='data:image/png;base64,lgr6ztKUG2d5oxti8A7BsEwTQissauXgWmiuMiojpatXF0dPRPw8PDG2zAwE9EG6LgbbAWAU8Qa0PjDbMBAU8QG6LgbbAWAU8Qa0PjDbMBAU8QG6LgbbAWAU8Qa0PjDbMBAU8QG6LgbbAWAU8Qa0PjDbMBAU8QG6LgbbAWAU8Qa0PjDbMBAU8QG6LgbbAWAU8Qa0PjDbMBgf8FKn6DbmfrMfYAAAAASUVORK5CYII=';
        // maskImage.src='data:image/png;base64,'
        // maskImage.src='data:image/png;base64,'
        // maskImage.src='data:image/png;base64,';
        //人形字符云
        maskImage.src="data:image/png;base64,"
            var option = {
                tooltip: {},
                series: [ {
                    type: 'wordCloud',
                    gridSize: 2,
                    sizeRange: [12, 55],
                    rotationRange: [-45,0,45,90],
                    // shape: 'pentagon',
                    maskImage:maskImage,
                    // width: 600,
                    // height: 400,
                    // drawOutOfBound: true,
                    textPadding:0,
                    autoSize:{
                      enable:true,
                      minSize:10
                    },
                    // drawOutOfBound: true,
                    textStyle: {
                        normal: {
                            color: function () {
                                return 'rgb(' + [
                                    Math.round(Math.random() * 160),
                                    Math.round(Math.random() * 160),
                                    Math.round(Math.random() * 160)
                                ].join(',') + ')';
                            }
                        },
                        emphasis: {
                            shadowBlur: 10,
                            shadowColor: '#333'
                        }
                    },
                    data: data
                } ]
            };
            maskImage.onload=function(){
              chart.setOption(option);
            }

            window.onresize = function(){
              chart.resize();
            }
